{
    "name": "Igracias Auto Survey",
    "version": "0.0.1",
    "browser_specific_settings": {
        "gecko": {
          "id": "lolywk@tutanota.com",
          "strict_min_version": "42.0"
        }
    },      
    "manifest_version": 3,
    "description": "Automagically fill igracias surveys",
    "permissions": ["activeTab", "scripting", "contextMenus", "storage"],
    "icons": {
        "16": "icons/igracias-icon.png",
        "48": "icons/igracias-icon.png",
        "128": "icons/igracias-icon.png"
    },
    "action": {
        "default_popup": "ui/index.html"
    },
    "background": {
        "scripts": ["background.js"]
    }
}
